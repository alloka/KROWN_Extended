{
    "@id": "http://example.org/mappings/20/1",
    "name": "mappings_20_1",
    "description": "Mappings 20TM + 1POMs",
    "steps": [
        {
            "@id": "http://example.org/mappings/20/1#step1",
            "name": "Load RDB",
            "resource": "PostgreSQL",
            "command": "load",
            "parameters": {
                "csv_file": "data.csv",
                "table": "data"
            },
            "expect_failure": false
        },
        {
            "@id": "http://example.org/mappings/20/1#step2",
            "name": "Execute RML mapping",
            "resource": "RMLMapper",
            "command": "execute_mapping",
            "parameters": {
                "mapping_file": "mapping.r2rml.ttl",
                "output_file": "out.nt",
                "serialization": "ntriples",
                "rdb_host": "PostgreSQL",
                "rdb_port": 5432,
                "rdb_username": "root",
                "rdb_password": "root",
                "rdb_type": "PostgreSQL",
                "rdb_name": "db"
            },
            "expect_failure": false
        }
    ]
}